<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
   <class name = "day5.entities.Course" table = "Course_Master">
      <id name = "courseId" type = "int" column = "course_id"/>         
      <property name = "name" column = "course_name" type = "string"/>
      <property name = "duration" column = "course_duration" type = "int"/>  
      <property name = "fees" column = "course_fees" type = "int"/>
      <property name = "provider" column = "course_provider" type = "string"/>   
   </class>
</hibernate-mapping>